{
    "name": "neurony/laravel-redirects",
    "description": "Nested URLs redirect logic for Laravel",
    "license": "MIT",
    "keywords": [
        "laravel",
        "redirect",
        "url",
        "slug",
        "seo"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Andrei Badea",
            "email": "zbiller@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/contracts": "^7.0",
        "illuminate/support": "^7.0",
        "illuminate/database": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Neurony\\Redirects\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Neurony\\Redirects\\Tests\\": "tests"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Neurony\\Redirects\\ServiceProvider"
            ]
        }
    }
}
